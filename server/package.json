{
  "name": "fullstack-test-server",
  "version": "1.0.0",
  "author": "Juan Pons",
  "description": "Server application ecosystem: NodeJs with typescript, REST api with express and routing controller + class-validator + DI + TypeOrm + ESLint + Prettier + Custom Cron scheduler + Custom Logger configs + Custom npm scripts. Webpack build configured",
  "license": "MIT",
  "scripts": {
    "doc:clean": "rm -rf docs",
    "doc": "npm run doc:clean && ./node_modules/.bin/typedoc --theme minimal --includeVersion --includeDeclarations --json docs/docs.json --mode file --categorizeByGroup true --exclude **/node_modules/** --out docs src",
    "cz": "git-cz",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --ext .js,.ts --fix",
    "pre-commit": "lint-staged",
    "pre-push": "npm run format-check",
    "build": "npm run webpack:build",
    "webpack:build": "per-env",
    "webpack:build:development": "webpack --env=development --mode=development",
    "webpack:build:staging": "webpack --env=staging --mode=production",
    "webpack:build:production": "webpack --env=production --mode=production",
    "webpack:watch": "webpack --env=development --mode=development --watch",
    "test": "npx mocha--require source-map-support/register --reporter spec",
    "start": "per-env",
    "start:development": "npm run webpack:watch",
    "start:staging": "npm run webpack:build:staging && node dist/server.js",
    "start:production": "npm run webpack:build:production && node dist/server.js",
    "typeorm": "./node_modules/ts-node/dist/bin.js -T --project tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create -n",
    "migrations:up": "npm run typeorm migration:run",
    "migrations:down": "npm run typeorm migration:revert",
    "migrations:show": "npm run typeorm migration:show",
    "schema:sync": "npm run typeorm  schema:sync",
    "schema:drop": "npm run typeorm  schema:drop"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "chai": "^4.2.0",
    "circular-dependency-plugin": "^5.2.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.0-rc.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "eslint-plugin-jsdoc": "^22.1.0",
    "express": "^4.18.2",
    "helmet": "^3.22.0",
    "joiful": "^2.0.0",
    "log4js": "^6.1.2",
    "method-override": "^3.0.0",
    "microframework": "^0.6.4",
    "mocha": "^7.1.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "per-env": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "rotating-file-stream": "^2.0.2",
    "routing-controllers": "^0.8.1-alpha.2",
    "routing-controllers-openapi": "^1.8.0",
    "ts-node": "^8.7.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.24",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.11",
    "@types/friendly-errors-webpack-plugin": "^0.1.2",
    "@types/joi": "^14.3.4",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.1",
    "@types/source-map-support": "^0.5.1",
    "@types/webpack": "^4.41.7",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-node-externals": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "chalk": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.5",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "log-symbols": "^3.0.0",
    "nodemon": "^2.0.2",
    "nodemon-webpack-plugin": "^4.3.1",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "source-map-support": "^0.5.16",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.17.4",
    "typedoc-plugin-markdown": "^2.2.17",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "engines": {
    "node": ">=10.3.0",
    "eslint": ">6.0.0",
    "typescript": ">3.0.0",
    "webpack": ">4.0.0"
  }
}
